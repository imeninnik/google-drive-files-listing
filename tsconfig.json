{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "sourceMap": true,
        "listFiles": false
    },
    "exclude": [
        "typings/browser",
        "typings/core-js",
        "typings/globals/es6-shim",
        "node_modules"
    ]
}
